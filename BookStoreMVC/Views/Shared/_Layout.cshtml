<!DOCTYPE html>
<html lang="en">

<head>
    @Html.SeoTags()
    <link rel="stylesheet" href="~/css/output.css" asp-append-version="true" />

</head>

<body class="font-inter bg-backGr">
    <!-- Navbar -->
    @Html.Partial("_NavbarPartial")
    <!-- End Nav -->

    <!--Main Content-->



    @* Sharethis Button *@
    <div class="sharethis-inline-share-buttons"></div>
    @* Sharethis Button *@
    <div class="mx-auto  min-h-screen ">
        @RenderBody()

    </div>

    <!--End Main Content-->


    <!--Footer-->
    @Html.Partial("_FooterPartial")
    <!--Footer-->

    <!-- Messenger Chat Plugin Code -->
    <div id="fb-root"></div>

    <!-- Your Chat Plugin code -->
    <div id="fb-customer-chat" class="fb-customerchat">
    </div>

    <!-- Messenger Chat Plugin Cod -->

    <script>
        var chatbox = document.getElementById('fb-customer-chat');
        chatbox.setAttribute("page_id", "107380592227881");
        chatbox.setAttribute("attribution", "biz_inbox");
    </script>

    <!-- Your SDK code -->

    <script src="https://js.stripe.com/v3/"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-252424466-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-252424466-1');
    </script>






    @* Sharethis*@
    <script type="text/javascript"
        src="https://platform-api.sharethis.com/js/sharethis.js#property=639ecabe4b058f001938ca18&product=inline-share-buttons&source=platform"
        async="async"></script>
    @* Sharethis*@




    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>